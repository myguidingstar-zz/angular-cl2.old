{
  "name": "angular-cl2",
  "version": "0.1.0",
  "description": "Angular macros for ChlorineJS",
  "scripts": {
    "watch": "cl2c -d test/test.cl2",
    "bare-compile": "cl2c -1 -b dev/main.cl2",
    "compile": "cl2c -1 -B dev/main.cl2",
    "test": "testem -f test/testem.json"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/chlorinejs/angular-cl2.git"
  },
  "keywords": [
    "angular",
    "qunit",
    "chlorinejs",
    "macro"
  ],
  "directories": {
    "lib":  "./lib"
  },
  "analyze": false,
  "devDependencies": {
    "chloric"  :  "0.1.9",
    "testem"   :  "*"
  },
  "license": "EPL",
  "engines": {
    "node": ">=0.8"
  }
}
